{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "./build",
      "allowJs": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "removeComments": true
   },
}